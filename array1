void Merge(int *ary,int *mergelist,int *left,int *mid,int *right)
    {
        int i,j,k;
        i=left;
        j=mid+1;
        k=left;
        while(i<mid&&j<=right)
        {
            if(ary[i]<ary[j])
                mergelist[k++]=ary[i++];
            else
            {
                mergelist[k++]=ary[j++];
            }
            if(i>mid)
                while(j<=right)
                    mergelist[k++]=ary[j++];
            else if(j>right)
                while(i<=mid)
                mergelist[k++]=ary[i++]


        }
    }
